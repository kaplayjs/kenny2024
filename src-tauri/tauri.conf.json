{
  "productName": "Lock To Lock",
  "version": "0.0.0",
  "identifier": "com.kaplayjs.kenny2024",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:4321",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Lock To Lock - KAPLAY",
        "width": 1280,
        "height": 720,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
